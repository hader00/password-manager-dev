{
  "name": "simple-password-manager-app",
  "productName": "Simple Password Manager",
  "version": "1.0.0",
  "private": true,
  "author": "Martin Haderka",
  "description": "Description",
  "license": "MIT",
  "main": "./public/electron.js",
  "homepage": "https://github.com/hader00/password-manager-dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/hader00/password-manager-dev"
  },
  "build": {
    "appId": "cz.martin.haderka.simple-password-manager-app",
    "files": [
      "build/**/*"
    ],
    "mac": {
      "category": "public.app-category.productivity",
      "target": "dmg",
      "publish": ["github"]
    },
    "linux": {
      "category": "Network",
      "publish": ["github"]
    },
    "win": {
      "publish": ["github"],
      "publisherName": "Martin Haderka"
    }
  },
  "dependencies": {
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "axios": "^0.24.0",
    "crypto-js": "^4.1.1",
    "electron-cookies": "^1.1.0",
    "electron-is-dev": "^2.0.0",
    "electron-store": "^8.0.1",
    "electron-window-state": "^5.0.3",
    "js-crypto-hkdf": "^1.0.4",
    "sqlite3": "^5.0.2",
    "validator": "^13.7.0",
    "ws": "^8.3.0"
  },
  "scripts": {
    "start": "concurrently \"cross-env BROWSER=none react-scripts start\" \"wait-on http://localhost:3000 && electron .\"",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "dist": "yarn build && electron-builder -mwl",
    "dist-mac": "yarn build && electron-builder -m",
    "dist-win": "yarn build && electron-builder -w",
    "dist-linux": "yarn build && electron-builder -l",
    "rebuild-electron": "electron-builder install-app-deps"
  },
  "devDependencies": {
    "concurrently": "^6.4.0",
    "cross-env": "^7.0.3",
    "electron": "^16.0.2",
    "electron-builder": "^23.0.2",
    "electron-rebuild": "^3.2.5",
    "node-gyp": "^8.4.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^4.0.3",
    "wait-on": "^6.0.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
